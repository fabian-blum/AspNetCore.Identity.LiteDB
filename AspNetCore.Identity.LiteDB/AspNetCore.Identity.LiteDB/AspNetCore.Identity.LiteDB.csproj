<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <PackageProjectUrl>https://github.com/fabian-blum/AspNetCore.Identity.LiteDB</PackageProjectUrl>
    <PackageLicenseUrl>https://github.com/fabian-blum/AspNetCore.Identity.LiteDB/blob/master/LICENSE</PackageLicenseUrl>
    <Authors>Fabian Blum</Authors>
    <Company />
    <Description>A LiteDB provider for ASP.NET Core Identity framework</Description>
    <PackageTags>AspNetCore Identity LiteDB</PackageTags>
    <Version>2.4.0</Version>
    <AssemblyVersion>2.4.0.0</AssemblyVersion>
    <FileVersion>2.4.0.0</FileVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="LiteDB" Version="3.1.4" />
    <PackageReference Include="Microsoft.AspNetCore.Identity.EntityFrameworkCore" Version="2.0.0" />
    <PackageReference Include="Newtonsoft.Json" Version="10.0.3" />
  </ItemGroup>

</Project>